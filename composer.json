{
    "name": "stfn/random-hash",
    "description": "This is my package random-hash",
    "keywords": [
        "stfn",
        "random-hash"
    ],
    "homepage": "https://github.com/stfn/random-hash",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Damjanovic",
            "email": "damjanovicstefan0@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laravel/pint": "^1.2",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Stfn\\RandomHash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stfn\\RandomHash\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
